<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="jjaworski" id="1">
        <createTable catalogName="book"
                     schemaName="public"
                     tableName="book">
            <column name="id" type="bigserial" autoIncrement="true" startWith="1"/>
            <column name="isbn" type="int"/>
            <column name="title" type="varchar(40)"/>
            <column name="author" type="varchar(40)"/>
            <column name="price" type="float"/>
            <column name="genre" type="varchar(40)"/>
            <column name="pubh" type="varchar(40)"/>
            <column name="description" type="varchar(255)"/>
        </createTable>
    </changeSet>
    <changeSet author="jjaworski" id="2">
